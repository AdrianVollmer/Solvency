{
  "name": "solvency",
  "version": "0.5.0",
  "private": true,
  "scripts": {
    "build:static": "node scripts/copy-static.js",
    "build:css": "npx tailwindcss -i src-frontend/css/input.css -o static/css/tailwind.css --minify && node scripts/hash-css.js",
    "build:ts": "node scripts/build-ts.js",
    "watch:css": "npx tailwindcss -i src-frontend/css/input.css -o static/css/tailwind.css --watch",
    "watch:ts": "node scripts/build-ts.js --watch",
    "generate:icons": "node scripts/generate-icons.js"
  },
  "devDependencies": {
    "esbuild": "^0.24.0",
    "lucide-static": "^0.562.0",
    "sharp": "^0.33.0",
    "sortablejs": "^1.15.6",
    "tailwindcss": "^3.4.0"
  },
  "dependencies": {
    "material-symbols": "^0.40.2"
  }
}
